    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.6.1",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "expect": "^28.1.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
